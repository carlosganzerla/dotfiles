{
  "mcpServers": {
    "chrome-devtools": {
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "command": "npx"
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    },
    "git": {
      "args": [
        "mcp-server-git"
      ],
      "command": "uvx",
      "disabled": true
    },
    "github": {
      "disabled": false,
      "headers": {
        "Authorization": "Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx",
      "disabled": false
    },
    "sentry": {
      "disabled": false,
      "url": "https://mcp.sentry.dev/mcp"
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "disabled": false
    },
    "stack-mcp-server": {
      "args": [
        "mcp-remote",
        "mcp.stackoverflow.com"
      ],
      "command": "npx",
      "disabled": true
    }
  }
}
